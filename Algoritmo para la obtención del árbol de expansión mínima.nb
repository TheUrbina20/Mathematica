Cell[BoxData[
 RowBox[{"\[IndentingNewLine]", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"obtenerListaPoligonos", "[", 
     RowBox[{"grafo_", ",", "dat_"}], "]"}], ":=", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", " ", 
       RowBox[{
       "g", ",", "listaDeVertices", ",", "coordenadas", ",", "poligonInd", 
        ",", "vertice1", ",", "vertice2", ",", "vertice3", ",", "vertice4", 
        ",", "coorx1", ",", "coorx2", ",", "coory1", ",", "coory2", ",", 
        "\[IndentingNewLine]", "auxiliar", ",", "  ", "datos", " ", ",", " ", 
        "poligonos"}], "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"g", "=", "grafo"}], ";", "\[IndentingNewLine]", 
       RowBox[{"datos", "=", "dat"}], ";", "\[IndentingNewLine]", 
       RowBox[{"listaDeVertices", " ", "=", " ", 
        RowBox[{"VertexList", "[", "g", "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"(*", 
        RowBox[{
        "LISTA", " ", "DE", " ", "COORDENADAS", " ", "DE", " ", "LOS", " ", 
